{
    "manifestVersion": 1,
    "id": "past",
    "version": "1.0.3",
    "name": "PowerApps Solution Settings Transform",
    "publisher": "AidanCole",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Transform the JSON settings file generated by PowerApps CLI with pipeline variables.",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "Power Apps",
        "Power Apps CLI",
        "ALM",
        "Application Lifecycle Management",
        "JSON",
        "PipelineTasks",
        "Deployment",
        "Solutions",
        "Pipeline",
        "Variables"
    ],
    "icons": {
        "default": "assets/logo.png"
    },
    "branding": {
        "color": "rgb(198, 198, 198)"
    },
    "links": {
        "support": {
            "uri": "https://github.com/a1dancole/PowerApps-Solution-Settings-Transform/issues/new?assignees=&labels=bug&template=bug_report.md&title=",
            "version": "1.0.0"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/a1dancole/PowerApps-Solution-Settings-Transform"
    },
    "screenshots": [

    ],
    "content": {
        "details": {
            "path": "assets/overview.md"
        }
    },
    "files": [
        {
            "path": "task"
        },
        {
            "path": "assets/overview.md",
            "addressable": true
        },
        {
            "path": "assets/logo.png",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}